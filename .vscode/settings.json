{
	//EDITOR
	"editor.autoClosingBrackets": "always",
	"editor.autoClosingQuotes": "beforeWhitespace",
	"editor.autoIndent": "keep",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.dragAndDrop": true,
	"editor.fontSize": 14,
	"editor.fontFamily": "JetBrains Mono",
	"editor.fontLigatures": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.guides.bracketPairs": true,
	"editor.guides.highlightActiveBracketPair": true,
	"editor.guides.highlightActiveIndentation": "always",
	"editor.guides.indentation": true,
	"editor.hover.enabled": true,
	"editor.inlineSuggest.enabled": true,
	"editor.lineHeight": 26,
	"editor.links": true,
	"editor.matchBrackets": "always",
	"editor.minimap.autohide": true,
	"editor.renderWhitespace": "all",
	"editor.semanticHighlighting.enabled": false,
	"editor.showDeprecated": true,
	"editor.showUnused": true,
	"editor.tabCompletion": "on",
	"editor.tabSize": 4,
	"editor.tokenColorCustomizations": {
		"comments": "#666ed4bd"
	},
	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 120,
	"editor.detectIndentation": true,
	//TERMINAL
	"terminal.integrated.fontFamily": "MesloLGM NF",
	"terminal.integrated.automationProfile.linux": "zsh",
	//EXPLORE
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": true,
	"explorer.copyRelativePathSeparator": "/",
	"explorer.decorations.colors": true,
	"explorer.decorations.badges": true,
	//WORKBENCH
	"workbench.colorCustomizations": {
		"editor.lineHighlightBackground": "#1073cf2d",
		"editor.lineHighlightBorder": "#9fced11f",
		"statusBar.background": "#93e733",
		"statusBar.foreground": "#070705",
		"statusBar.noFolderBackground": "#7bd657",
		"editorIndentGuide.activeBackground1": "#E02970"
	},

	"workbench.editor.enablePreview": false,
	"workbench.editor.labelFormat": "short",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.startupEditor": "none",
	"workbench.tree.renderIndentGuides": "always",
	"workbench.tree.indent": 20,
	//GIT
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	//copilot
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": false,
		"scminput": false,
		"yaml": false,
		"javascript": true,
		"typescript": true,
		"javascriptreact": true,
		"typescriptreact": true,
		"json": true,
		"jsonc": true,
		"html": true,
		"css": true
	},
	//GIT LENS
	"gitlens.defaultDateShortFormat": null,
	"gitlens.defaultTimeFormat": null,
	"gitlens.codeLens.recentChange.enabled": false,
	"gitlens.codeLens.authors.enabled": false,
	"gitlens.codeLens.enabled": false,
	//Tabnine
	"tabnine.experimentalAutoImports": true,
	//PRETTIER
	"prettier.arrowParens": "always",
	"prettier.bracketSameLine": true,
	"prettier.jsxSingleQuote": true,
	"prettier.printWidth": 80,
	"prettier.semi": false,
	"prettier.singleQuote": true,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
	"prettier.singleAttributePerLine": true,
	"prettier.proseWrap": "always",
	//OTHERS
	"auto-rename-tag.activationOnLanguage": ["*"],
	"breadcrumbs.enabled": true,
	"cSpell.language": "en,de-de",
	"cSpell.userWords": [
		"capmec",
		"createjournal",
		"messquick",
		"NEXTAUTH",
		"pinheiro",
		"postit",
		"prisma",
		"Signup",
		"Tabnine",
		"whitelogo"
	],

	"typescript.updateImportsOnFileMove.enabled": "always",
	"rapidapi.terminalLink.enabled": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"scm.diffDecorationsIgnoreTrimWhitespace": "true",
	"files.trimTrailingWhitespace": true,
	"[dockercompose]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"polacode.transparentBackground": true,
	"projectManager.git.baseFolders": [
		"C:\\Users\\c.pinheiro\\Documents\\Messquick"
	],
	"typescript.tsdk": "node_modules\\typescript\\lib",
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"cSpell.words": ["journalnumber"],
	"workbench.colorTheme": "Monokai Vibrant"
}
